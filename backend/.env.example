# Backend Environment Variables
NODE_ENV=development
PORT=3001

# Database (will be provided by Cloud SQL)
DATABASE_URL=postgresql://localhost:5432/analytics_dev

# Redis (will be provided by Memorystore)
REDIS_URL=redis://localhost:6379

# JWT Secret
JWT_SECRET=your_jwt_secret_here

# Google Cloud Configuration
GOOGLE_SERVICE_ACCOUNT_KEY=your_google_service_account_key_here
GOOGLE_SERVICE_ACCOUNT_EMAIL=your_service_account@your_project.iam.gserviceaccount.com
GOOGLE_PROJECT_ID=your_google_project_id

# Storage
DATA_BUCKET=analytics-platform-data
ASSETS_BUCKET=analytics-platform-assets

# External APIs
SMTP_HOST=localhost
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASS=your_smtp_password

# Copy this file to .env and replace the placeholder values with your actual configuration